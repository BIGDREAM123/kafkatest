<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lxq.springkafka02.mapper.StudentMapper">
    <insert id="insertStudent">
        insert into student(student_name,student_age,student_id,student_sex,class_id)
        values(#{studentName},#{studentAge},#{studentId},#{studentSex},#{classId})
    </insert>
</mapper>